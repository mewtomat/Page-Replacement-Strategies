#PUBLICDIR= /usr0/cs564/public/project
SRC= buf.c hash.c pf.c
OBJ= buf.o hash.o pf.o
HDR = pftypes.h pf.h 

pflayer.o: $(OBJ)
	ld -r -o pflayer.o $(OBJ)

tests: testhash testpf

testpf: testpf.o pflayer.o
	cc -o testpf testpf.o pflayer.o

mytestpf: mytestpf.o pflayer.o
	cc -o mytestpf mytestpf.o pflayer.o

testhash: testhash.o pflayer.o
	cc -o testhash testhash.o pflayer.o

$(OBJ): $(HDR)

testhash.o: $(HDR)

testpf.o: $(HDR)

mytestpf.o: $(HDR)

lint: 
	lint $(SRC)

install: pflayer.o 

clean:
	rm -f *.o file1 file2

join_mytestpf: join_mytestpf.o pflayer.o
	cc -o join_mytestpf join_mytestpf.o pflayer.o
	
join_mytestpf.o: $(HDR)


select_mytestpf: select_mytestpf.o pflayer.o
	cc -o select_mytestpf select_mytestpf.o pflayer.o
	
select_mytestpf.o: $(HDR)
